  },
  "devDependencies": {
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.1.21"
  },
  "scripts": {
    "docs": "typedoc --theme markdown --excludeExternals --externalPattern \"**/node_modules/**\" --mode file --ignoreCompilerErrors --out docs src",
